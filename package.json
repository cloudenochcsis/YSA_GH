{
  "name": "ysa-ghana-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:host": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "nuxt typecheck",
    "clean": "rm -rf .nuxt dist .output",
    "clean:win": "rmdir /s /q .nuxt dist .output 2>nul || echo Clean completed",
    "docker:build": "docker build -t ysa-ghana:latest .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t ysa-ghana:dev .",
    "docker:run": "docker run -p 3000:3000 ysa-ghana:latest",
    "docker:run-dev": "docker run -p 3000:3000 -v $(pwd):/app ysa-ghana:dev",
    "docker:compose-dev": "docker-compose up ysa-ghana-dev",
    "docker:compose-prod": "docker-compose --profile production up ysa-ghana-prod"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "@nuxt/content": "^2.13.4",
    "@nuxt/fonts": "^0.9.2",
    "@nuxt/icon": "^1.5.5",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "^2.18.7",
    "@pinia/nuxt": "^0.5.5",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "nuxt": "^3.13.2",
    "pinia": "^2.2.8",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/test-utils": "^3.15.3",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vue-tsc": "^3.0.5"
  }
}
